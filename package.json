{
    "name": "lucario-vscode-theme",
    "version": "0.0.3",
    "displayName": "Lucario Theme",
    "description": "VSCode port of the Lucario theme by Raphael Amorim. A darkish theme for the 80% use case.",
    "publisher": "victor-gp",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/victor-gp/lucario-vscode-theme"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "dark",
        "darkish",
        "comfortable",
        "pokemon"
    ],
    "icon": "assets/icon.png",
    "galleryBanner": {
        "color": "#2b3e50",
        "theme": "dark"
    },
    "bugs": "https://github.com/victor-gp/lucario-vscode-theme/issues",
    "qna": "false",
    "contributes": {
        "themes": [
            {
                "label": "Lucario",
                "uiTheme": "vs-dark",
                "path": "./themes/Lucario-color-theme.json"
            }
        ]
    },
    "engines": {
        "vscode": "^1.64.0"
    },

    "scripts": {
        "build": "node ./scripts/build.js",
        "prewatch": "npm run build",
        "watch": "node ./scripts/watch.js"
    },
    "devDependencies": {
        "js-yaml": "^4.1.0"
    }
}
